{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "build",
      "command": "/usr/bin/gcc",
      "args": [
        "-o",
        "${workspaceRoot}/clox.o",
        "-g",
        "${workspaceRoot}/*.c"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": [
        "$gcc"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "Task generated by Debugger."
    }
  ],
  "version": "2.0.0"
}
